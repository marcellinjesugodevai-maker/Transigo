{
    "framework": "nextjs",
    "buildCommand": "next build",
    "installCommand": "pnpm install",
    "outputDirectory": ".next",
    "headers": [
        {
            "source": "/apks/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache, no-store, must-revalidate"
                },
                {
                    "key": "Pragma",
                    "value": "no-cache"
                },
                {
                    "key": "Expires",
                    "value": "0"
                }
            ]
        }
    ]
}