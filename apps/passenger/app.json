{
  "expo": {
    "name": "TransiGo",
    "slug": "transigo-passager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "transigo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF3E0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.transigo.passenger",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "TransiGo a besoin de votre position pour trouver des chauffeurs pr√®s de vous.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "TransiGo a besoin de votre position pour suivre votre trajet."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFF3E0"
      },
      "package": "com.transigo.passenger"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "expo-location",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": false,
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "newArchEnabled": false
          }
        }
      ],
      "./plugins/withFixedReactNativePath"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "05993757-fc16-43d7-b6f3-7ef0617907d9"
      }
    },
    "owner": "devai123"
  }
}