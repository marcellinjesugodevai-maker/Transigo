{
  "expo": {
    "name": "TransiGo",
    "slug": "transi-go-passenger",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-v2.png",
    "scheme": "transigo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF3E0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.transigo.passenger",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "TransiGo a besoin de votre position pour trouver des chauffeurs pr√®s de vous.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "TransiGo a besoin de votre position pour suivre votre trajet."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-v2.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.transigo.passenger2",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "geo"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "maps.google.com"
            },
            {
              "scheme": "https",
              "host": "www.google.com",
              "pathPrefix": "/maps"
            },
            {
              "scheme": "https",
              "host": "goo.gl",
              "pathPrefix": "/maps"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "expo-location",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": false,
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "newArchEnabled": false
          }
        }
      ],
      "./plugins/withFixedReactNativePath"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "952fce41-2cb4-41a4-8658-2cad036f7056"
      }
    },
    "owner": "marceljesugo"
  }
}